@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_CRMLayout.cshtml";
}


<h1>Index</h1>

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>

<div align="center" ;>
    <button id="btn1" class="btn btn-success" style="display:inline-block; inline-size:150px">Button 1</button>
    <br />
    <br />
    <button id="btn2" class="btn btn-warning" style="display:inline-block; inline-size:150px">Button 2</button>
</div>

<script>$("#btn1").click(function () {
        swal("Hello world!");
    })
    $("#btn2").click(function () {
        swal("Good job!", "You clicked the button!", "success");
    })
</script>
