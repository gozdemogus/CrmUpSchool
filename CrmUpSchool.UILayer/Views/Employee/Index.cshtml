@model List<CrmUpSchool.EntityLayer.Concrete.Employee>

@{
    Layout = "~/Views/Shared/_CrmLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Personel</th>
        <th>Departman</th>
        <th>Sil</th>
        <th>Düzenle</th>
        <th>Görevler</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmployeeID</td>
            <td>@item.EmployeeName @item.EmployeeSurname</td>
            <td>@item.Category.CategoryName</td>
            <td><a href="#" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn btn-outline-success">Güncelle</a></td>
            <td><a href="#" class="btn btn-outline-dark">Görevler</a></td>
        </tr>
    }
</table>
<a href="#" class="btn btn-primary">New Personel</a>